<h2><%= @category.name  %> Search Results</h2>

<% if @errors %>
  <%= @errors %>
<% end %>

<h3>Database Results</h3>
<% if @database_results %>
  <ul>
    <% @database_results.each do |result| %>
      <li><%=result.task %></li>
    <% end %>
  </ul>
<% end %>

<h3>API Results</h3>
<% if @api_results %>
  <ul>
    <% @api_results.each do |result| %>
      <li><%=result %></li>
    <% end %>
  </ul>
<% end %>
